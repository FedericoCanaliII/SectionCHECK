    border-top-left-radius: 4px;
}

/* Ultima sezione dellâ€™header (ultima colonna) con angolo in alto a destra stondato */
QHeaderView::section:last {
    border-top-right-radius: 4px;
}

/* Rendo la vertical header (colonna 0) visivamente assente impostandone larghezza a 0 */
